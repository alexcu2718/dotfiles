// Place your key bindings in this file to override the defaults
[
    {
      "key": "alt+h", // key to press to activate command
      "command": "settings.cycle", 
      "when": "editorTextFocus", // this keybinding is only active when (editor is in focus)
      "args": { // these are the arguments passed to `settings.cycle`
        "id": "toggle-language-inlay-hints", // must be unique
        "overrideWorkspaceSettings": true,
        "values": [ // Note: use the same settings in each values object
          {
            "editor.inlayHints.enabled": false
          },
          {
            "editor.inlayHints.enabled": true
          }
        ]
      }
    },
    {
      "key": "ctrl+k",
      "command": "editor.action.deleteLines",
      "when": "textInputFocus && !editorReadonly"
    }, 

      {
    "key": "shift+r shift+a",
    "command": "rust-analyzer.restartServer"
  },



      {
    "key": "shift+up",
    "command": "editorScroll",
    "when": "textInputFocus",
    "args": {
      "to": "up",
      "by": "line",
      "value": 15,     
      "revealCursor": false
    }
  },
  // Scroll down by 10 lines when Shift + Down is pressed
  {
    "key": "shift+down",
    "command": "editorScroll",
    "when": "textInputFocus",
    "args": {
      "to": "down",
      "by": "line",
      "value": 10,
      "revealCursor": false
    }
  },

      {
    "key": "ctrl+shift+a",
    "command": "editor.action.selectAll",
    "when": "editorTextFocus"
  },


 

     {
    "key": "ctrl+a",
    "command": "cursorHome",
    "when": "textInputFocus"
  },  
     {
    "key": "shift+left",
    "command": "workbench.action.previousEditor",
    "when": "editorTextFocus"
  },
  {
    "key": "shift+right",
    "command": "workbench.action.nextEditor",
    "when": "editorTextFocus"
  },
  {
    "key": "ctrl+e",
    "command": "cursorEnd",
    "when": "textInputFocus"
  },

  {
    "key": "shift+tab",
    "command": "outdent",
    "when": "editorTextFocus && !editorReadonly"
},

  {
    "key": "ctrl+=",
    "command": "jupyter.insertCellBelow",
    "when": "notebookEditorFocused"
},

